<!DOCTYPE html>
<html>
	<%- include('./partials/_head');%>
	<body>
	<div class="container-fluid">
	<%- include('./partials/_header'); %>
	<%- include('./partials/_bootstrapJS'); %>
    
	<h1>Channels</h1>
	<div class="col-md-12 col-lg-3">
		<ul class="list-group">
			<% channel_list.forEach(function(channel){ %>
				<a href=<%-'channels/'+channel.name%> class="list-group-item list-group-item-action">
					<li class="d-flex align-items-center">
						#<%= channel.name %>
					</li>
					
				</a>
			<% }); %>
		</ul>
	</div>
	</div>
  </body>
</html>
